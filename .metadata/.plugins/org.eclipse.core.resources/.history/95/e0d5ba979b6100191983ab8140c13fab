package com.internousdev.final_login.dto;

public class LoginDTO {

	String name;
	String pass;

	LoginDTO(String name, String pass){
		this.name = name;
		this.pass = pass;
	}

	public String getName(){
		return this.name;
	}
	public void setName(String name){
		this.name = name;
	}
	public String getPass(){
		return this.pass;
	}
	public void setPass(String pass){
		this.pass = pass;
	}

}
