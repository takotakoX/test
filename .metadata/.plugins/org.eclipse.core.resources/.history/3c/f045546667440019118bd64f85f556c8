
public class HelloWorld {

	public static void main(String[] args) {
		String[][] country=new String[3][2];
		
		country[0][0]="japan":
		country[0][1]="thailand":
		country[1][0]="us":
		country[1][1]="brazil":
		country[2][0]="france":
		country[2][1]="russia":
		
		System.out.println(country[0][0]);
	}
}
