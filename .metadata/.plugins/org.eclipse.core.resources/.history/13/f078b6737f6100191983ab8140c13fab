package practice;

public class Practice17 {
	public static void main(String[] args){
		System.out.println("test");
		StringBuilder sb = new StringBuilder();
		for(int i=0;i<100;i++){
			sb.append(i+1).append(",");
		}
		String s = sb.toString();
		String[] a = s.split(",");
		System.out.println("配列");
		System.out.println(a);
	}

}
