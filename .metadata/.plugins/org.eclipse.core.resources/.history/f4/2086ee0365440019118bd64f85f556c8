
public class HelloWorld {

	public static void main(String[] args) {
		int a=110;
		
		if(a<=100){
			if(a<20){
				System.out.println("未成年");
			}else if(a==70){
				System.out.println("喜寿");
			}else if(a==88){
				System.out.println("米寿");
			}else{
				System.out.println("成人");
			}
		}
		
	}
}

